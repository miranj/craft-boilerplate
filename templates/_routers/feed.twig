{#
  RSS Feed Router
#}

{%- extends (entry is instance of ('craft\\elements\\Category') ? [
  '_views/feeds/' ~ entry.group.handle,
] : [
  '_views/feeds/' ~ entry.section.handle ~ '-' ~ entry.type.handle,
  '_views/feeds/' ~ entry.section.handle,
  '_views/feeds/_' ~ entry.type.handle,
])|merge([
  '_views/feeds/_default',
]) -%}
