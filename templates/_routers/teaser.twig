{#
  Teaser Router
#}

{%- extends (element is instance of ('craft\\elements\\Category') ? [
  '_views/teasers/' ~ element.group.handle,
] : [
  '_views/teasers/' ~ element.section.handle ~ '-' ~ element.type.handle,
  '_views/teasers/' ~ element.section.handle,
  '_views/teasers/_' ~ element.type.handle,
])|merge([
  '_views/teasers/_default',
]) -%}
