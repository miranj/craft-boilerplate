{% extends '_schemas/details/default' %}

{% set schemaProps = {
  listItems: pageEntries ?? [],
  numberOfItems: pageInfo.total ?? (pageEntries ?? [])|length ?: null,
}|merge(schemaProps ?? {}) %}



{% block entry_schema %}
  {% from '_schemas/definitions/itemList' import itemList %}
  {% do graph.add(itemList(schemaProps)) %}
  
  {{ parent() }}
{% endblock %}
