{% extends '_layouts/base' %}



{#
  Render
#}
{% block main_class 'container' %}
{% block main %}
  {# Show the most recently edited entries #}
  {{ ul(
    craft.entries
      .uri(':notempty:')
      .orderBy('dateUpdated desc')
      .all()
      |column('link'),
    { encode: false },
  ) }}
{% endblock %}
